{
  "name": "nordcloudproblem",
  "version": "1.0.0",
  "description": "This is an interpretation of the problem proposed by Nordcloud to calculate the most suitabe 'link station'\r for any given point.\r This link station should have the highest possible power as per specifications.",
  "main": "index.js",
  "scripts": {
    "test": "npm run env:test && jest",
    "start": "npm run compile && cross-env NODE_ENV=dev node ./babel_compiled/main.js",
    "env:dev": "cross-env NODE_ENV=dev",
    "env:test": "cross-env NODE_ENV=test",
    "env:production": "cross-env NODE_ENV=production",
    "compile": "babel src --out-dir babel_compiled/src",
    "lambda:export": "babel index.js --out-dir babel_compiled && npm run compile",
    "delete:exports": "RD /S babel_compiled",
    "parcel:build:main": "parcel build ./src/main.js",
    "parcel:build:lambda": "parcel build  ./src/*/** index.js",
    "parcel:clear": "RD /S /Q dist",
    "build:lambda": "google-closure-compiler --compilation_level=bundle --dependency_mode=PRUNE_LEGACY --js=index.js --js=src/** --js_output_file=out.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jamikon/NordcloudProblem.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jamikon/NordcloudProblem/issues"
  },
  "homepage": "https://github.com/Jamikon/NordcloudProblem#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "google-closure-compiler": "^20210106.0.0",
    "jest": "^26.6.3"
  }
}
